set(OcraYarp_VERSION @OcraYarp_VERSION@)

@PACKAGE_INIT@

if(NOT TARGET OcraYarp::ocra-control)
  include("${CMAKE_CURRENT_LIST_DIR}/OcraYarpTargets.cmake")
endif()

find_package(YARP REQUIRED)
find_package(YCM REQUIRED)
find_package(iDynTree REQUIRED)
find_package(yarpWholeBodyInterface REQUIRED)
find_package(OcraRecipes REQUIRED)
find_package(TinyXML REQUIRED)

set(OcraYarp_LIBRARIES "@OcraYarp_TARGETS@" "${Boost_LIBRARIES};${TinyXML_LIBRARIES};${YARP_LIBRARIES}")
set(OcraYarp_INCLUDE_DIRS "@PACKAGE_OcraYarp_INCLUDEDIR@" "${Boost_INCLUDE_DIRS};${TinyXML_INCLUDE_DIRS};${YARP_INCLUDE_DIRS};${Eigen_INCLUDE_DIRS};${EigenLgsm_INCLUDE_DIRS}")
